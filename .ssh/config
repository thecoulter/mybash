Host glycamvm
     HostName 172.16.2.180
     User root
     ForwardAgent yes
     HostKeyAlgorithms +ssh-rsa
     PubkeyAcceptedKeyTypes +ssh-rsa     
     # Ciphers 3des-cbc
     # KexAlgorithms +diffie-hellman-group1-sha1
     ProxyJump frost
     ServerAliveInterval 240
     
