# sshuttle proxy
#e.g. alias 'proxyhome'='proxy uselr hostdomain:22 192.168.1.0/24'
function proxy() {
  screen -S proxy -dm sshuttle -v -r $1@$2 -x $2 ${@:2}; screen -r proxy
}

