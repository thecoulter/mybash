function legacyvnc() {

case $1 in
    ''|*[!0-9]*) echo "enter the last digit of port number" ; return 1 ;;
    *) screen -S portforward -dm ssh -v -N root@172.16.0.80 -J andrew@128.192.9.183 -L 590$1:localhost:590$1 ;;
esac
}

